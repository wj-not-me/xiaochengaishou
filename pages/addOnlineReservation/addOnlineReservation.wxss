.panel-choose {
  width: 690rpx;
  background: #fff;
  border-radius: 8rpx;
  margin-top: 20rpx;
  margin-left: 30rpx;
}


.choose-recyclingaAddress {
  height: 119rpx;
  border-bottom: 1rpx solid rgba(233,234,236,1);
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
