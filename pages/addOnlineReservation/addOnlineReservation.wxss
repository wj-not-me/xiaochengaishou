}

.chosen-recyclingAddress {
  box-sizing: border-box;
  padding: 35rpx 30rpx;
  border-bottom: 1rpx solid rgba(233,234,236,1);
  word-break: break-all;
  overflow: hidden
}

.chosen-contact {
  width: 550rpx;
  /* margin-left: 30rpx;
  margin-top: 25rpx; */
