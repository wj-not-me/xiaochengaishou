  margin-top: 20rpx;
  background: #fff;
}

.panel-statistics {
  width: 690rpx;
  border-radius: 8rpx 8rpx 8rpx 8rpx;
  background: #fff;
  margin-top: 20rpx;
  margin-left: 30rpx;
}

.statistics-title {
  height: 79rpx;
  border-bottom: 1rpx solid rgba(233,234,236,1);
  line-height: 79rpx;
  font-size: 30rpx;
  font-weight: 400;
  color: rgba(28,36,56,1);
}

.statistics-title text {
  margin-left: 30rpx;
}

.statistics-content {
  padding: 30rpx;
}

.statistics-content-item {
  height: 26rpx;
  display: flex;
  justify-content: space-between;
  margin-bottom: 20rpx;
  line-height: 26rpx;
  font-size: 26rpx;
  font-weight: 400;
}

.statistics-content-item:last-child {
