.panel-recyclingTypes-detail {
  width: 690rpx;
  border-bottom: 1rpx solid rgba(233,234,236,1);
  background: #fff;
  margin-left: 30rpx;
  display: flex;
  flex-direction: column;
  font-size: 26rpx;
  font-weight: 400;
  color: rgba(128,132,143,1);
}

.recyclingTypes-detail-title, .recyclingTypes-detail-item {
  width: 100%;
  box-sizing: border-box;
  padding: 10rpx 30rpx;
  display: flex;
  align-items: center;
}

.recyclingTypes-detail-title {
  color: rgba(28,36,56,1);
}

.detail-title-name, .detail-item-name {
  width: 50%;
  text-align: center;
}

.detail-title-price, .detail-item-price {
  width: 50%;
  text-align: center;
}

