  width: 12rpx;
  height: 20rpx;
  float: right;
  /* margin-top: 35rpx; */
  /* margin-right: 30rpx; */
}

.chosen-contact>view {
  line-height: 30rpx;
}

.chosen-contact>view:first-child {
  max-width: 330rpx;
  margin-right: 20rpx;
