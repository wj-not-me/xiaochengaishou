}

.statistics-footer {
  box-sizing: border-box;
  height: 80rpx;
  padding: 15rpx 30rpx 20rpx 3rpx;
  border-top: 1rpx solid rgba(233,234,236,1);
  display: flex;
  justify-content: flex-end;
  align-items: baseline;
}

.statistics-footer-title {
  font-size: 26rpx;
  font-weight: 400;
  color:rgba(73,80,96,1);
}

.statistics-footer-content {
  margin-left: 7rpx;
  font-size: 36rpx;
  font-weight: 400;
  color:rgba(255,123,35,1);
}

.reOnlineReservation, .cancleOnlineReservation {
  width: 100vw;
